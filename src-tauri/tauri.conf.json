{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "set dns",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "http": {
        "scope": [
          "https://api.telegram.org/*"
        ]
      },
      "fs": {
        "all": false,
        "readFile": true,
        "writeFile": true,
        "readDir": true,
        "createDir": true,
        "exists": true,
        "removeFile": true,
        "removeDir": true,
        "scope": [
          "$APPDATA/*",
          "$HOME/.idns/**"
        ]
      },
      "dialog": {
        "all": true,
        "ask": true,
        "confirm": true,
        "message": true,
        "open": true,
        "save": true
      },
      "process": {
        "all": true,
        "exit": true,
        "relaunch": true,
        "relaunchDangerousAllowSymlinkMacos": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.rust.idns",
      "longDescription": "idns Desktop Application",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "Simple To Set DNS",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "webviewInstallMode": {
          "silent": true,
          "type": "embedBootstrapper"
        }
      }
    },
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": false
    },
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": false,
      "endpoints": [
        "https://cdn.jsdelivr.net/gh/itxve/idns@gh-pages/install.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDhDQTI3QUU4QkZCNjUyNzYKUldSMlVyYS82SHFpakZRYmNMVm5oSnYyemVHV1BSemUxeEZTMXhJZ1ZHbnB5aTJzK2lYejFKc2oK",
      "windows": {
        "installMode": "passive"
      }
    }
  }
}